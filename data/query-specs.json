            "id": "page--revisions",
            "status": "done",
            "comment": "Show a sparkline when other WebPage with the same url were posted.",
            "name": "Revisions",
            "query": "anchored_timeline",
            "sparql": "?item a s:WebPage ; s:url URI ; s:dateCreated ?category .",
            "elastic": {
              "type": "WebPage",
              "anchor_path": "url",
              "filter": {
                "term": {
                  "url": "ANCHOR"
                }
              },
              "date_histogram": {
                "field": "dateCreated",
                "interval": "week"
              }
            }
          }
        ]
      }
